public static void main(String[] args) {
		String str="aacdb";
		String str1="gafd";
		str=str+str1;
		char[]array=str.toCharArray();
		for(int i=0;i<str.length();i++)
		{
			for(int j=i+1;j<str.length();j++)
			{
				if(str.charAt(i)==str.charAt(j))
				{
					array[i]='@';
					array[j]='@';
				}
	

			}
		}
		for(int i=0;i<array.length;i++)
		{
			if(array[i]!='@')
			{
				System.out.print(array[i]);
			}
		}
	}

	

	
	}
