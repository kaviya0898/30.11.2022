public static void main(String[] args) {
		int arr[]= {8,5,10,7,9,4,15,12,90,13};
		int k=4;
		for(int i=0;i<arr.length;i++)
		{
			if(i+k-1==arr.length)
			{
				break;
			}
			int max=0;
			for(int j=i;j<=i+k-1;j++)
			{
				if( j<arr.length  &&  max<arr[j])
				{
					max=arr[j];
				}
			}
			System.out.print(max+" ");
			
		}
		
	}
